parameters:
  level: 9
  tmpDir: '%currentWorkingDirectory%/.phpstan'
  paths:
    - src
    - tests/Phpstan
  fileExtensions:
    - php
  parallel:
    processTimeout: 60.0
    maximumNumberOfProcesses: 4
  ignoreErrors:
    - '#should return TemirkhanN\\Generic\\ResultInterface<.+?> but returns TemirkhanN\\Generic\\Result<mixed>#'
